  "result_list_rendering": {
    "url": "(item) => { return `https://openlibrary.org/${item.work_id}` }",
    "body": "(item) => { return item.description }",
    "image": "(item) => { return item.cover ? `https://covers.openlibrary.org/b/id/${item.cover}-M.jpg` : null }",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return`${item.subtitle ? item.subtitle + ' | ' : ''} ${item.authors.join(', ')} | ${item.publishers ? item.publishers[0] : ''} | ${item.first_publish_year || 'year unknown'}` }",
    "required_fields": [
      "title",
      "subtitle",
      "authors",
      "publishers",
      "first_publish_year",
      "description",
      "cover",
      "work_id"
    ]
  },
  "hover_label_rendering": {
    "image": "(item) => { return null }",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return`${item.authors.join(', ')} | ${item.publishers ? item.publishers[0] : ''} | ${item.first_publish_year || 'year unknown'}` }",
    "required_fields": [
      "title",
      "subtitle",
      "authors",
      "publishers",
      "first_publish_year",
      "description",
      "cover_thumbnail",
      "work_id"
    ]
  },
  "detail_view_rendering": {
    "url": "(item) => { return `https://openlibrary.org/${item.work_id}` }",
    "body": "(item) => { return `<b>Description:</b> ${item.description || 'n.a.'}<br><b>Subjects:</b> ${item.subjects}<br><b>Genres:</b> ${item.genres}<br><b>Series:</b> ${item.series}` }",
    "image": "(item) => { return item.cover ? `https://covers.openlibrary.org/b/id/${item.cover}-M.jpg` : null }",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return`${item.subtitle ? item.subtitle + ' | ' : ''} ${item.authors.join(', ')} | ${item.publishers ? item.publishers[0] : ''} | ${item.first_publish_year || 'year unknown'}` }",
    "required_fields": [
      "title",

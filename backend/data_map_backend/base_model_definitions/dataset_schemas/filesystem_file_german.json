{
  "name": "Filesystem File (German)",
  "entity_name": "file",
  "entity_name_plural": "files",
  "short_description": "Files uploaded from a file system structure",
  "created_at": "2024-03-05T14:44:12.758Z",
  "changed_at": "2024-11-01T15:59:19.857Z",
  "primary_key": "full_path",
  "thumbnail_image": null,
  "descriptive_text_fields": [
    "title",
    "abstract",
    "full_path"
  ],
  "default_search_fields": [
    "title",
    "abstract",
    "full_text",
    "full_text_chunk_embeddings",
    "embedding_e5"
  ],
  "advanced_options": {
    "range_filters": [],
    "retrieval_mode": "hybrid",
    "ranking_options": [
      {
        "title": "Relevance only",
        "tooltip": "Rank by similarity to query only"
      },
      {
        "title": "Relevant & recent documents",
        "tooltip": "Rank mostly by relevance, but slightly favor recent documents",
        "boost_function": {
          "functions": [
            {
              "exp": {
                "created_at": {
                  "decay": 0.5,
                  "scale": "1000d"
                }
              }
            }
          ]
        }
      },
      {
        "sort": [
          {
            "created_at": {
              "order": "desc",
              "missing": "_last"
            }
          }
        ],
        "title": "Newest first",
        "tooltip": "Strictly sort by creation date, as long as the document contains the keywords at least once"
      }
    ],
    "map_vector_field": "embedding_e5",
    "export_button_name": "Export",
    "language_filtering": {
      "field": "language",
      "format": "iso_639_set_1",
      "options": [
        "en",
        "de"
      ]
    },
    "default_result_language": "de",
    "full_text_chunk_embeddings": "full_text_chunk_embeddings",
    "ignored_keyword_highlight_fields": [
      "title",
      "abstract"
    ]
  },
  "result_list_rendering": {
    "url": "item => null",
    "body": "item => `${item.abstract} <br><br>${item.language} - ${item.ai_tags ? item.ai_tags.join(', ') : ''}`",
    "icon": "item => icon_for_file_suffix(item.file_type)",
    "image": "item => get_download_url(item.thumbnail_path)",
    "title": "item => `${item.title}`",
    "tagline": "item => `${item.file_name}`",
    "subtitle": "item => `${item.type_description} ${item.content_date ? '- ' + item.content_date : ''} ${item.content_time ? '- ' + item.content_time : ''}`",
    "sub_tagline": "item => `${item.folder ? item.folder.replaceAll('/', ' \u203a ') : ''}`",
    "required_fields": [
      "title",
      "abstract",
      "thumbnail_path",
      "language",
      "content_date",
      "content_time",
      "ai_tags",
      "type_description",
      "file_type",
      "file_name",
      "folder"
    ]
  },
  "hover_label_rendering": {
    "image": "(item) => { return null }",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return null }",
    "required_fields": [
      "title"
    ]
  },
  "detail_view_rendering": {
    "url": "item => null",
    "body": "item => `${item.abstract} <br><br>${item.language} - ${item.ai_tags ? item.ai_tags.join(', ') : ''}`",
    "icon": "item => icon_for_file_suffix(item.file_type)",
    "image": "item => get_download_url(item.thumbnail_path)",
    "links": [
      {
        "url": "item => get_download_url(item.uploaded_file_path)",
        "label": "Download File"
      }
    ],
    "title": "item => `${item.title}`",
    "tagline": "item => `${item.file_name}`",
    "subtitle": "item => `${item.type_description} ${item.content_date ? '- ' + item.content_date : ''} ${item.content_time ? '- ' + item.content_time : ''}`",
    "sub_tagline": "item => `${item.folder ? item.folder.replaceAll('/', ' \u203a ') : ''}`",
    "required_fields": [
      "title",
      "abstract",
      "thumbnail_path",
      "language",
      "content_date",
      "content_time",
      "ai_tags",
      "type_description",
      "file_type",
      "file_name",
      "uploaded_file_path",
      "folder"
    ]
  },

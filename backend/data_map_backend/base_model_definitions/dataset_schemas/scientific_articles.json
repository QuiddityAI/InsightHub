  "result_list_rendering": {
    "url": "(item) => { return null }",
    "body": "item => item.abstract",
    "image": "item => get_download_url(item.thumbnail_path)",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return `${item.publication_year || 'year unknown'}, ${item.authors?.slice(0, 80)}` }",
    "required_fields": [
      "title",
      "journal",
      "authors",
      "publication_year",
      "abstract",
      "thumbnail_path"
    ]
  },
  "hover_label_rendering": {
    "url": "(item) => { return null }",
    "body": "(item) => { return null }",
    "image": "(item) => { return null }",
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return item.authors.slice(0, 40) }",
    "required_fields": [
      "title",
      "authors",
      "publication_year"
    ]
  },
  "detail_view_rendering": {
    "doi": "(item) => { return item.DOI }",
    "url": "(item) => { return null }",
    "body": "item => `${item.abstract}\nDOI: ${item.doi}`",
    "image": "item => get_download_url(item.thumbnail_path)",
    "links": [
      {
        "url": "item => get_download_url(item.file_path)",
        "label": "Open PDF"
      }
    ],
    "title": "(item) => { return item.title }",
    "subtitle": "(item) => { return `${item.publication_year || 'year unknown'}, ${item.authors?.slice(0, 80)}` }",
    "required_fields": [
      "title",
      "journal",
      "authors",
      "publication_year",
      "abstract",
      "doi",
      "file_path",
      "thumbnail_path"
    ],
    "full_text_pdf_url": "item => get_download_url(item.file_path)"
  },
